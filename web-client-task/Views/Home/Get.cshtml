@using web_client_task.ViewModels
@model GetProductsViewModel

<table class="table">
		<tr>
		<th>
			Product Name
		</th>
		<th>
			Product Description
		</th>
		<th>
			Product Default Quantity
		</th>
	</tr>
	@foreach(var item in Model.ProductDtos)
	{
	<tr>
		<td>
			@item.Name
		</td>
		<td>
			@item.Description
		</td>
		<td>
			@item.DefaultQuantity
		</td>
	</tr>
	}
	</table>